{
  "name": "hasbase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "cp .env.web .env.local && vite",
    "dev:mobile": "cp .env.mobile .env.local && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "cp .env.desktop .env.local && tauri dev -- --profile dev-fast",
    "tauri:android:dev": "cp .env.mobile .env.local && tauri android dev",
    "tauri:android:build": "cp .env.mobile .env.local && tauri android build --apk --split-per-abi",
    "tauri:ios:dev": "tauri ios dev",
    "server": "bun run --watch bun-server/index.ts",
    "build:tauri": "cp .env.desktop .env.local &&  bun run tauri build --bundles app --verbose",
    "build:sidecar-winos": "bun build --compile --target=bun-windows-x64-v1.2.2 --minify --sourcemap ./src/backend/index.ts --outfile src-tauri/bin/api/main-x86_64-pc-windows-msvc",
    "build:sidecar-macos": "bun build --compile --minify --sourcemap ./src/backend/index.ts --outfile src-tauri/bin/api/main-aarch64-apple-darwin",
    "build:sidecar-linux": "bun build --compile --minify --sourcemap ./src/backend/index.ts --outfile src-tauri/bin/api/main-x86_64-unknown-linux-gnu",
    "build:icons": "bun tauri icon public/app-icon.png",
    "dev:api": "bun run --watch src/backend/index.ts",
    "api:test-bin": "./src-tauri/bin/api/main-aarch64-apple-darwin"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.16",
    "@electric-sql/pglite-react": "^0.2.15",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/node": "^1.2.5",
    "@elysiajs/stream": "^1.1.0",
    "@elysiajs/swagger": "^1.2.0",
    "@lancedb/lancedb": "^0.15.0",
    "@langchain/anthropic": "^0.3.11",
    "@langchain/community": "^0.3.28",
    "@langchain/core": "^0.3.37",
    "@langchain/deepseek": "^0.0.1",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/ollama": "^0.1.4",
    "@langchain/openai": "^0.4.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.62.8",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "@tauri-apps/plugin-sql": "~2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "elysia": "^1.1.26",
    "framer-motion": "^11.15.0",
    "konsta": "^4.0.1",
    "lucide-react": "^0.469.0",
    "node-json-db": "^2.3.1",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router": "^7.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "tinybase": "^5.4.7",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tauri-apps/cli": "^2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.2.2",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.39",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}