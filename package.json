{
  "name": "hasbase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "cp .env.web .env.local && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "cp .env.desktop .env.local && tauri dev",
    "tauri:android:dev": "PLATFORM=mobile tauri android dev",
    "tauri:ios:dev": "tauri ios dev",
    "server": "bun run --watch server/index.ts",
    "build:tauri": "bun run tauri build --bundles dmg",
    "build:server": "bun build server/index.ts --target=bun-darwin-arm64 --outfile src-tauri/binaries/app-server-aarch64-apple-darwin --compile",
    "build:server2": "bun build server/index.ts --outfile src-tauri/binaries/app --compile",
    "test": "bun build server/test.ts --compile --outfile server/app"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.15",
    "@electric-sql/pglite-react": "^0.2.15",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.62.8",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "elysia": "^1.1.26",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.39",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
